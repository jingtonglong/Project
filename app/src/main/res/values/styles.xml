<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/app_color</item>
        <item name="colorPrimaryDark">@color/app_color</item>
        <item name="colorAccent">@color/app_color</item>
    </style>

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--导航栏的颜色-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!--通知栏等的颜色-->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!--字体的主题颜色-->
        <item name="android:textColorPrimary">@color/black</item>
        <!--解决textView全局大小写问题-->
        <item name="android:textAllCaps">false</item>
        <!-- toolbar中带动画效果的箭头，设置动画形式与箭头颜色 -->
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <!-- toolbar中menu的文字的颜色,并非溢出菜单的内容文字颜色 -->
        <item name="actionMenuTextColor">@color/white</item>
        <item name="android:windowAnimationStyle">@style/pageInOut</item>
    </style>

    <!-- 页面跳转间的过渡动画 -->
    <style name="pageInOut">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_from_right</item>

        <item name="android:activityOpenExitAnimation">@anim/slide_out_to_left</item>

        <item name="android:activityCloseEnterAnimation">@anim/slide_in_from_left</item>

        <item name="android:activityCloseExitAnimation">@anim/slide_out_to_right</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppNoTitle" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Splash" parent="AppNoTitle">
        <item name="android:windowBackground">@drawable/splash_layer</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!-- 左边的箭头指示-->
